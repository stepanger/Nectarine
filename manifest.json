{
  "manifest_version": 2, 
  "name": "Nectarine", 
  "version": "0.1",
  "description": "Nectarine",
  "browser_action": {
    "default_title": "Nectarine",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
        "src/background/history.js"
    ],
    "persistent": true
  },
  "short_name": "Nectarine",
  "permissions": [
    "history",
    "storage"
  ],
   "content_scripts":[
    {
      "js":[
          "build/jquery-1.11.3.min.js",
          "src/content_scripts/app.js"
      ],
      "css":[
        "css/mystyles.css"
      ],
      "matches":[
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ]
}